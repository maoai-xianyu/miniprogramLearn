/* pages/flexdemo/flexdemo.wxss */

.outter {
    display: flex;
    justify-content: space-evenly;
    width: 600rpx;
    height: 400rpx;
    background: pink;
}

.outter .inner {
    background: gray;
    width: 100rpx;
    height: 100rpx;
    border: 10rpx solid #e4e4e4;
    /* 边框隐藏在盒子里面 */
    box-sizing: border-box;
}